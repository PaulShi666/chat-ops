<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sse.chatops.dao.MessageMapper">

    <select id="selectMessageByText" resultType="com.sse.chatops.model.Message">
        SELECT * FROM message WHERE text LIKE #{text}
    </select>
</mapper>